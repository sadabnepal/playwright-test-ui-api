{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@ui/data/*": [
        "tests/ui/data/*"
      ],
      "@fixtures/*": [
        "tests/ui/fixtures/*"
      ],
      "@ui/interface/*": [
        "tests/ui/interfaces/*"
      ],
      "@pages/*": [
        "tests/ui/pages/*"
      ],
      "@api/data/*": [
        "tests/api/data/*"
      ],
      "@api/helper/*": [
        "tests/api/helper/*"
      ],
      "@api/schema/*": [
        "tests/api/schema/*"
      ],
      "@env/*": [
        "tests/env/*"
      ]
    }
  }
}