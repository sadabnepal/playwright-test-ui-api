{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./tests",
    "paths": {
      "@ui/data/*": [
        "./ui/data/*"
      ],
      "@fixtures/*": [
        "./ui/fixtures/*"
      ],
      "@ui/interface/*": [
        "./ui/interfaces/*"
      ],
      "@pages/*": [
        "./ui/pages/*"
      ],
      "@api/data/*": [
        "./api/data/*"
      ],
      "@api/helper/*": [
        "./api/helper/*"
      ],
      "@api/schema/*": [
        "./api/schema/*"
      ],
      "@env/*": [
        "./env/*"
      ]
    }
  }
}